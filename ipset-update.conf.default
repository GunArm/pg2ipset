# config for ipset-update.sh


ENABLE_LOGGING=1
LOG_FILE="/var/log/ipset-update.log"

# enable iblocklist lists?
ENABLE_IBLOCKLIST=1
# list activation in /etc/blocklists/iblocklist.lists


# enable country blocks?
ENABLE_COUNTRY=0

# countries to block, must be lcase
COUNTRIES=(af ae ir iq tr cn sa sy ru ua hk id kz kw ly)


# enable tor blocks?
ENABLE_TORBLOCK=0

# ports to block tor users from
PORTS=(80 443 6667 22 21)
